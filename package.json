{
  "name": "gitbook-plugin-lazyload",
  "version": "0.1.0",
  "description": " A gitbook plugin to lazyload image inspired by lightbox.",
  "main": "dist/index.js",
  "repository": "https://github.com/LemonPrefect/gitbook-plugin-lazyload.git",
  "author": "LemonPrefect <me@lemonprefect.cn>",
  "license": "MIT",
  "keywords": [
    "gitbook",
    "plugin",
    "lazyload",
    "image"
  ],
  "engines": {
    "gitbook": ">2.x.x"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "copyfiles": "~2.1.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "gitbook-cli": "^2.3.2",
    "lazyload.js": "^1.0.2"
  },
  "scripts": {
    "lint": "eslint src/*.js",
    "build": "npm run babel && npm run copy:lazyload",
    "babel": "babel src --out-dir dist --presets=@babel/env",
    "copy:lazyload": "copyfiles -f node_modules/lazyload.js/lazyload.min.js dist/assets/js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "gitbook": {}
}
